[user]
	name = Ezra
	email = ezragocci@gmail.com
[init]
	defaultBranch = main
[pull]
	rebase = true
[push]
	default = current
	autoSetupRemote = true
[help]
	autocorrect = 5
[alias]
	st = status
	co = checkout
	br = branch
	ci = commit
	lg = log --oneline --graph --decorate --all
	last = log -1 HEAD
[core]
	pager = delta
[interactive]
	diffFilter = delta --color-only
[delta]
	navigate = true
	side-by-side = true
[merge]
	conflictstyle = diff3
[diff]
	colorMoved = default
